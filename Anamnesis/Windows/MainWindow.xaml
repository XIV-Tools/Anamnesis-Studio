<XivToolsWpfWindows:ChromedWindow
	x:Class="Anamnesis.Windows.MainWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:XivToolsWpfWindows="clr-namespace:XivToolsWpf.Windows;assembly=XivToolsWpf"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:Anamnesis.Windows"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="{Binding Title, UpdateSourceTrigger=LostFocus}"
	d:DesignHeight="450"
	d:DesignWidth="800"
	Background="{DynamicResource BackgroundBrush}"
	mc:Ignorable="d">
	<Grid>
		<ProgressBar
			Width="300"
			IsIndeterminate="True"
			Style="{StaticResource XivToolsWaiter}" />
	</Grid>
</XivToolsWpfWindows:ChromedWindow>
