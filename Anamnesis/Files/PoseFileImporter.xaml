<files:ActorFileImporterBase
	x:Class="Anamnesis.Files.PoseFileImporter"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:controls="clr-namespace:Anamnesis.Controls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:files="clr-namespace:Anamnesis.Files"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>

		<Grid.RowDefinitions>
			<RowDefinition />
			<RowDefinition />
		</Grid.RowDefinitions>

		<controls:ActorSelector
			Grid.Row="0"
			Grid.ColumnSpan="2"
			Margin="0,0,0,3"
			Selection="{Binding Actor}" />



	</Grid>
</files:ActorFileImporterBase>
